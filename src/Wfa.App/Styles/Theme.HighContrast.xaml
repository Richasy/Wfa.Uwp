<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="AppMaskAcrylicBrush" Color="{ThemeResource SystemChromeAltMediumHighColor}" />
    <SolidColorBrush x:Key="AccentMaskAcrylicBrush" Color="{ThemeResource SystemAccentColor}" />
    <SolidColorBrush x:Key="AppMaskBrush" Color="{ThemeResource SystemChromeAltMediumHighColor}" />
    <SolidColorBrush x:Key="AccentMaskBrush" Color="{ThemeResource SystemColorWindowColor}" />
    <SolidColorBrush x:Key="AccentBadgeBackground" Color="{ThemeResource SystemColorWindowColor}" />
    <SolidColorBrush x:Key="ToggleSwitchKnobOffForeground" Color="White" />
    <SolidColorBrush x:Key="TransparentBackground" Color="#01000000" />
    <LinearGradientBrush x:Key="ImageMaskGradientBackground" StartPoint="0,0.5" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#01000000" />
            <GradientStop Offset="1" Color="#cc000000" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>
